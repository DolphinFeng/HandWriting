{
  "name": "scheduleweb",
  "version": "1.0.0",
  "offlineVersion": "1.0.9",
  "scripts": {
    "analyze": "ANALYZE=1 ANALYZE_MODE=server onejs build",
    "build": "onejs build",
    "cdn": "cross-env ./node_modules/.bin/mss-deploy ./mssconfig.js",
    "ci": "npm run lint && onejs cov",
    "cov": "onejs cov",
    "dev": "onejs dev",
    "devs": "MOCK=none onejs dev",
    "help": "onejs help",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "precommit": "npm run lint",
    "start": "pm2-runtime ./server/pm2.json",
    "start:local": "PLUS_TEMPLATE_ENV=local nodemon ./server/index.js",
    "test": "onejs test",
    "uploadDXOfflineFiles": "node -r esm ./bin/uploadDXOfflineFiles.js",
    "sync": "i18n sync -c i18n.config.json",
    "i18n": "rm -rf temp/locales && yarn i18next -c i18next.config.js",
    "i18n:search": "node ./bin/i18n-search.js -c ./src",
    "i18n:excel": "node ./bin/jsonToExcel.js"
  },
  "dependencies": {
    "@ant-design/colors": "^7.0.0",
    "@block/babel-plugin-istanbul": ">=1.0.0",
    "@dp/lion-client": "3.1.7",
    "@dp/pike-message-web": "^2.2.6",
    "@it/citadel-open-sdk": "^1.1.1",
    "@mtfe/hlb": "^1.0.0",
    "@mtfe/koa2-security": "^0.2.2",
    "@mtfe/mns-util": "^2.0.0",
    "@mtfe/mt-config": "^3.0.1",
    "@mtfe/mtracer": "^0.1.11",
    "@mtfe/sso-client": "3.3.0",
    "@mtfe/uni-logger": "^0.1.1",
    "@onejs/gray-node": "^0.3.0",
    "@onejs/koa-logger": "^0.0.5",
    "@onejs/onejs": "4.0.6-beta.12",
    "@sailor/i18n-resource-sync": "^0.2.1",
    "@sailor/i18n-web": "^0.2.1",
    "@ss/mtd-react": "1.1.0-beta.8",
    "@xm/dxopen-client": "3.4.1",
    "babel-plugin-lodash": "^3.3.4",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.5",
    "eslint-plugin-prettier": "^3.1.2",
    "i18next-parser": "^8.7.0",
    "lodash": "4.17.15",
    "lodash.merge": "^4.6.0",
    "mobx": "^5.x",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.4",
    "path-to-regexp": "^6.2.1",
    "rc-slider": "^10.1.0",
    "react-draggable": "^4.4.3",
    "react-mutation-observer": "^1.1.0",
    "react-stretchable": "^1.1.2"
  },
  "devDependencies": {
    "@ant-design/colors": "^7.0.0",
    "@onejs/eslint-config-all": "^0.0.3",
    "@onejs/mss-deploy": "1.0.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.20.0",
    "babel-register": "^6.18.0",
    "cross-env": "^5.0.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^3.1.0",
    "json2xls": "^0.1.2",
    "koa": "^2.0.0",
    "koa-body": "^2.3.0",
    "koa-favicon": "^2.0.0",
    "koa-router": "^7.1.0",
    "koa-send": "^5.0.0",
    "koa-static": "^3.0.0",
    "pm2": "^2.4.0",
    "uglify-js": "^3.17.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.css": "stylelint --fix",
    "*.scss": "stylelint --syntax=scss --fix",
    "*.less": "stylelint --syntax=less --fix",
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "install-node": "16"
  }
}
